extend schema @link(url: "https://specs.apollo.dev/federation/v2.4", import: ["@key", "@shareable"])

scalar EnvironmentID

extend type Query {
    environment(id: EnvironmentID!): Environment
}

type Environment @key (fields: "id") {
    id: EnvironmentID!

    description: String
    createdAt: Time!
    modifiedAt: Time
}
